<hr>
<div id="rooms-container">
  <h3 id='my-id' data-id={{userId}}>My Rooms Dashboard</h3>
  {{!-- admin rooms --}}
  {{#if iAdmin}}
  <p>Rooms I Moderate:</p>
  {{#each iAdmin}}
  <ul id="admin-rooms">
    <li id={{id}}><a href="/rooms/{{id}}" target="_self">{{name}}</a></li>
  </ul>
  {{/each}}
  {{/if}}
  {{!-- chatting rooms --}}
  {{#if iChat}}
  <p>Participant Rooms:</p>
  {{#each iChat}}
  <ul id="chat-rooms">
    <li id={{id}}><a href="/rooms/{{id}}" target="_self">{{name}}</a><span> hosted by {{participant.0.username}}</span></li>
  </ul>
  {{/each}}
  {{/if}}
</div>
